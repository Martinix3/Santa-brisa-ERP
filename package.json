{
  "name": "santa-brisa-app",
  "version": "1.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cross-env GOOGLE_IMPERSONATE_SERVICE_ACCOUNT=firebase-adminsdk-fbsvc@santa-brisa-erp.iam.gserviceaccount.com next dev",
    "build": "next build",
    "start": "next start -H 0.0.0.0 -p 3000",
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.typecheck.json --noEmit",
    "dev:sa": "cross-env GOOGLE_IMPERSONATE_SERVICE_ACCOUNT=firebase-adminsdk-fbsvc@santa-brisa-erp.iam.gserviceaccount.com next dev",
    "smoke:run": "tsx src/smoke.ts",
    "test": "vitest",
    "test:exports": "vitest run tests/ssot-exports.test.ts",
    "clean": "rm -rf .next",
    "genkit:start": "genkit start",
    "genkit:run": "genkit run",
    "genkit:list": "genkit list",
    "audit:static": "node scripts/audit.js",
    "audit:runtime": "node scripts/audit-runtime.mjs",
    "audit": "npm run audit:static && npm run audit:runtime",
    "analyze": "ANALYZE=true npm run build",
    "check-budgets": "node scripts/check-budgets.cjs",
    "worker": "tsx src/server/queue/runner.local.ts",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "validate": "npm run typecheck && npm run lint && npm run test && node scripts/find-route-collisions.mjs && node scripts/check-budgets.cjs",
    "sanity:write": "tsx scripts/sanity-write.ts"
  },
  "dependencies": {
    "@dnd-kit/core": "latest",
    "@fullcalendar/core": "latest",
    "@fullcalendar/daygrid": "latest",
    "@fullcalendar/interaction": "latest",
    "@fullcalendar/list": "latest",
    "@fullcalendar/react": "latest",
    "@fullcalendar/timegrid": "latest",
    "@genkit-ai/ai": "latest",
    "@genkit-ai/googleai": "latest",
    "@google-cloud/firestore": "latest",
    "firebase": "latest",
    "firebase-admin": "latest",
    "firebase-functions": "^5.0.1",
    "framer-motion": "latest",
    "genkit": "latest",
    "jszip": "latest",
    "lucide-react": "latest",
    "next": "latest",
    "papaparse": "latest",
    "pdf-lib": "1.17.1",
    "puppeteer": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-dropzone": "latest",
    "recharts": "latest",
    "server-only": "latest",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "latest",
    "@next/bundle-analyzer": "latest",
    "@tailwindcss/postcss": "latest",
    "@types/estree": "latest",
    "@types/json-schema": "latest",
    "@types/node": "latest",
    "@types/papaparse": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "autoprefixer": "latest",
    "cross-env": "^7.0.3",
    "esbuild": "latest",
    "eslint": "^9.8.0",
    "eslint-config-next": "latest",
    "madge": "8.0.0",
    "postcss": "latest",
    "tailwindcss": "latest",
    "tsx": "latest",
    "typescript": "latest",
    "vite": "latest",
    "vite-node": "latest",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "latest"
  },
  "engines": {
    "node": ">=20",
    "npm": ">=10.0.0"
  },
  "resolutions": {
    "zod": "^3.23.8"
  }
}
