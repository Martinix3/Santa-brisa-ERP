{
  "name": "santa-brain-functions",
  "engines": {
    "node": "20"
  },
  "type": "module",
  "private": true,
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "serve": "npm run build && firebase emulators:start --only functions,firestore",
