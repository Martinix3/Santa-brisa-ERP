{
  "name": "santa-brain-functions",
  "engines": { "node": "20" },
  "type": "module",
  "private": true,
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "serve": "npm run build && firebase emulators:start --only functions,firestore",
    "deploy": "npm run build && firebase deploy --only functions:santaBrain",
    "genkit:dev": "genkit start --functions",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@genkit-ai/ai": "^0.9.0",
    "@genkit-ai/googleai": "^0.9.0",
    "firebase-admin": "^12.6.0",
    "firebase-functions": "^5.1.1",
    "zod": "^3.23.8",
    "cross-fetch": "^4.0.0"
  },
  "devDependencies": {
    "typescript": "^5.5.4",
    "@types/node": "^20.12.12",
    "@types/firebase-functions": "^3.18.0"
  }
}
